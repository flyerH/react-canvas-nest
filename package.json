{
  "name": "react-canvas-nest",
  "version": "1.1.1-beta.0",
  "description": "React component for a nest backgroud",
  "main": "./dist/react-canvas-nest.min.js",
  "scripts": {
    "start": "npm run dev",
    "build": "webpack --config ./config/webpack.conf.build.js",
    "prod": "rimraf example/dist/* && (webpack --config ./config/webpack.conf.prod.js)",
    "dev": "rimraf dist/* && (npm run build -- --watch --devtool cheap-module-eval-source-map & webpack-dev-server --color --config ./config/webpack.conf.dev.js )"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flyerH/react-canvas-nest.git"
  },
  "keywords": [
    "canvas",
    "nest",
    "canvas-nest",
    "react-nest"
  ],
  "author": "flyerH",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/flyerH/react-canvas-nest/issues"
  },
  "homepage": "https://github.com/flyerH/react-canvas-nest#readme",
  "peerDependencies": {
    "react": ">=15.0.0",
    "react-dom": ">=15.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "css-loader": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.3",
    "react": "15.0.0",
    "react-dom": "15.0.0",
    "rimraf": "^2.6.2",
    "terser-webpack-plugin": "^2.2.2",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.9"
  }
}
